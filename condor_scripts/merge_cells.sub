executable = merge_cells.sh
arguments = $(ClusterID) $(ProcId) $(PLATENUMBER)
output = output/mergecells.$(ClusterId).$(ProcId).out
error = error/mergecells.$(ClusterId).$(ProcId).err
log = log/mergecells.$(ClusterId).log
+JobFlavour = "tomorrow"
request_memory = 80GB
notification = Never
queue PLATENUMBER in 1
